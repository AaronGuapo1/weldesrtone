
let suma = 0;

for (let i=0; i<MaterialesProductos.length; i++){

    if (MaterialesProductos[i].familia === "Aceros"){


         suma = suma + (MaterialesProductos[i].preciounitario * MaterialesProductos[i].cantidad)
         if (i=MaterialesProductos.length){
            console.log(suma)
    
            }
    }
}



