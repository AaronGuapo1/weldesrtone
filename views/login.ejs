        <div class="limiter">
            <div class="container-login100">
                <div class="wrap-login100">

                    <form action="/logearse" method="POST" enctype="multipart/form-data" class="login100-form validate-form">
                        <!-- Logo -->
                        <span class="login100-form-title p-b-48">
                            <img class="logo" src="assets/brand/logo.png" alt="Welderstone Logo">
                        </span>
                        
                        <!-- Input Nombre de Usuario -->
                        <div class="wrap-input100 validate-input" data-validate = "Valid email is: a@b.c">
                            <input class="input100" type="text" name="username" id="username" required>
                            <span class="focus-input100" data-placeholder="Nombre de Usuario"></span>
                        </div>
                        
                        <!-- Input Contraseña -->
                        <div class="wrap-input100 validate-input" data-validate="Enter password">
                            <span class="btn-show-pass">
                                <i class="zmdi zmdi-eye"></i>
                            </span>
                            <input class="input100" type="password" id="password" name="password" required>
                            <span class="focus-input100" data-placeholder="Contraseña"></span>
                        </div>
                        
                        <!-- Boton Iniciar Sesion -->
                        <div class="container-login100-form-btn">
                            <div class="wrap-login100-form-btn">
                                <div class="login100-form-bgbtn"></div>
                                <button type="submit" class="login100-form-btn">
                                    Iniciar Sesión
                                </button>
                            </div>
                        </div>
                        
                        <!-- Link a Registrarse -->
                        <div class="text-center p-t-115">
                            <span class="txt1">
                                ¿No tienes una cuenta?
                            </span>
    
                            <a class="txt2" href="/registrarse">
                                Registrarse
                            </a>
                        </div>
                    </form>
                </div>
            </div>
        </div>

